{
    "name": "onphp/onphp",
    "description": "onPHP is the mature GPL'ed multi-purpose object-oriented PHP framework.",
    "keywords": ["framework"],
    "homepage": "https://github.com/onPHP/onphp-framework",
    "type": "library",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Konstantin Arkhipov"
        },
        {
            "name": "Anton Lebedevich"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "conflict": {
        "ext-http": "*"
    },
    "autoload": {
        "psr-4": {
            "onphp\\": "src/"
        },
        "files": ["src/global.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "onphp\\test\\": "test/"
        }
    }
}
